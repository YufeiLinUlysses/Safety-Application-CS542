t albersProjection = d3\r\n        .geoAlbers()\r\n        .scale(190000)\r\n        .rotate([71.057, 0])\r\n        .center([0, 42.313])\r\n        .translate([w/2, h/2]);\r\n\r\n      const geoPath = d3.geoPath().projection(albersProjection);\r\n\r\n      // Plug the graph into the region\r\n      const g = svg.append(\"g\");\r\n\r\n      g.selectAll(\"path\")\r\n        .data(boston.features)\r\n        .enter()\r\n        .append(\"path\")\r\n        .attr(\"fill\", \"#ccc\")\r\n        .attr(\"stroke\", \"#333\")\r\n        .attr(\"d\", geoPath);\r\n    },\r\n  },\r\n};\r\n</script>"]}]}