 default {\r\n  name: \"App\",\r\n  components: {\r\n    GChart,\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      chartsLib: null,\r\n      chartData: [],\r\n      show: true,\r\n      chartOptions: {\r\n        chartArea: { width: \"100%\", height: \"90%\" },\r\n        legend: {\r\n          position: \"top\",\r\n        },\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    onChartReady(chart, google) {\r\n      this.chartsLib = google;\r\n    },\r\n    createGraph() {\r\n      var vm = this;\r\n      var url = \"/locCTypeCount\";\r\n      try {\r\n        webcall\r\n          .post(url, vm.$store.state.location)\r\n          .then(async function (response) {\r\n            var temp = await JSON.parse(JSON.stringify(response.data));\r\n            console.log(temp);\r\n            if (temp.length <= 1 || temp.length == undefined) {\r\n              vm.show = false;\r\n            } else {\r\n              var result = [[\"DESCRIPTION\", \"NUM\"]];\r\n              temp[5][\"DESCRIPTION\"] = \"OTHER\";\r\n              for (var i = 6; i < temp.length; i++) {\r\n                temp[5][\"NUM\"] =\r\n                  parseInt(temp[i][\"NUM\"]) + parseInt(temp[5][\"NUM\"]);\r\n                temp.splice(i, 1);\r\n                i--;\r\n              }\r\n              for (var j of temp) {\r\n                var cur = [];\r\n                cur.push(j[\"DESCRIPTION\"]);\r\n                cur.push(parseInt(j[\"NUM\"]));\r\n                result.push(cur);\r\n              }\r\n              // alert(result)\r\n              // console.log(result)\r\n              // vm.chartData = result.slice(0,11)\r\n              vm.chartData = result;\r\n            }\r\n          });\r\n      } catch (err) {\r\n        console.log(\"error\");\r\n        alert(err);\r\n      }\r\n    },\r\n  },\r\n  mounted() {\r\n    this.createGraph();\r\n  },\r\n};\r\n</script>"],"sourceRoot":"src/components/NearByGraphs"}]}